package strategy.sorting_strategy;

public abstract class SorterAbstract implements Sorter {
    public abstract void sort(int[] arr);
    public void printArr(int[] arr) {
        int n = arr.length;
        for (int i = 0; i < n; ++i)
            System.out.print(arr[i] + " ");
        System.out.println("\n");
    }
}
