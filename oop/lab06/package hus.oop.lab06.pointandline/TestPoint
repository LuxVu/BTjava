package hus.oop.lab06.pointandline;

public class TestPoint {
    public static void main(String[] args) {
        Point p1 = new Point(10, 20);   // construct a Point
        System.out.println(p1);

        // try setting p1 to (100, 10);
        p1.setXY(100, 10);
        System.out.println(p1);
    }
}
